#include<stdio.h>
#include<stdlib.h>
int main(void)
{ int x,y,z;
char op;
scanf("%d%d%c",&x,&y,&op);
switch (op)
{
case '+':
	printf("The Addition of the two numbers is Equal %d",x+y);
break;
case '*':
	printf("The Multiplication of the two numbers is Equal %d",x*y);
break;
case '/':
printf("The Division of the two numbers is Equal %d",x/y);
break;
default:
	printf("invalid Operation");
}




}

