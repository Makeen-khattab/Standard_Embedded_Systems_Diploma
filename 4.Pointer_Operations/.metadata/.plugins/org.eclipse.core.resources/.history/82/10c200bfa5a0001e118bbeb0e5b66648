#include<stdio.h>
#include<stdlib.h>
void Sort_Bubb(int*);//void Sort(int[]);
void Swap(int*,int*);
#define MAX_SIZE 20
int Odd(int*,int);
int main(void)
{int arr_old[MAX_SIZE];




}
int Odd(int*Ptr2Arr,int Size)
{int i,c=0;
	Sort_Bubb(Ptr2Arr);
	for(i=0;i<Size-1;++i)
	{ if(Ptr2Arr[i]==Ptr2Arr[i+1])
	{
		c++;
	}
	else if(c%2)
	{
		return Ptr2Arr[i];
	}
	else
		c=0;



	}


}
void Sort_Bubb(int*Ptr2Arr)
{int i,j;
for(i=0;i<MAX_SIZE-1;++i)
	for(j=0;j<MAX_SIZE-1-i;++j)
	{
	if(Ptr2Arr[j]>Ptr2Arr[j+1])
		Swap(Ptr2Arr+j,Ptr2Arr+j+1);
	}
}
void Swap(int*v,int*u)
{int Temp;
Temp=*v;
*v=*u;
*u=Temp;
}
