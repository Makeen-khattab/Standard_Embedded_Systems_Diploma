#include<avr/io.h>
#include<util/delay.h>
int main(void)
{
/* Application initialization*/
DDRD&=~(1<<2);
DDRC|=1<<1;
/* Super_Loop*/
while(1)
{
	if(!(PIND&(1<<2)))
	{
		PORTC|=1<<1;
	}
	else
		PORTC&=~(1<<1);




}








}




