#include"button.h"
#include<avr/io.h>
#include "gpio.h"
int main(void)
{
/* Application initialization*/
DDRC|=1<<1;
DDRA&=~(1<<2);
/* Super_Loop*/
while(1)
{
	if( (PINA & (1<<2)) ==0)
	{
		PORTC|=1<<1;
	}
	else
		PORTC&=~(1<<1);




}








}




